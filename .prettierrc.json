{
  "importOrder": [
    "^(client-only|server-only)$",
    "<THIRD_PARTY_MODULES>",
    "^@/app/(.*)$",
    "^@/components/(.*)$",
    "^@/server/(.*)$",
    "^(@/styles/(.*))|(@/trpc/(.*))|(@/lib/(.*))$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-prisma",
    "prettier-plugin-sql"
  ],
  "language": "postgresql"
}
