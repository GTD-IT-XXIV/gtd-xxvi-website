    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
