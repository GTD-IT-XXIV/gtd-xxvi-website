{
  "name": "gtd-xxvi-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node .next/standalone/server.js",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "test": "vitest",
    "\n========== Development ==========": "",
    "dev": "next dev",
    "dev:prisma": "dotenv -c development -- prisma",
    "dev:stripe": "cross-env stripe listen --forward-to localhost:${PORT:-3000}/api/webhook",
    "dev:email": "email dev",
    "\n========== Development Database ==========": "",
    "dev:db:studio": "dotenv -c development -- prisma studio",
    "dev:db:start": "docker compose -p gtd-xxvi-website-dev -f docker/development/docker-compose.yaml up -d db",
    "dev:db:stop": "docker compose -p gtd-xxvi-website-dev -f docker/development/docker-compose.yaml down db",
    "dev:db:push": "dotenv -c development -- prisma db push",
    "dev:db:pull": "dotenv -c development -- prisma db pull",
    "dev:db:migrate": "dotenv -c development -- prisma migrate dev",
    "dev:db:clear": "dotenv -c development -- prisma migrate reset",
    "dev:db:init": "pnpm dev:db:push && pnpm dev:prisma db execute --file prisma/init-1-gtdfest.sql && pnpm dev:prisma db execute --file prisma/init-2-merch.sql",
    "dev:db:reset": "pnpm dev:db:clear && pnpm dev:db:init",
    "\n========== Development Build ==========": "",
    "dev:build": "docker compose -p gtd-xxvi-website-dev -f docker/development/docker-compose.yaml build",
    "dev:build:start": "docker compose -p gtd-xxvi-website-dev -f docker/development/docker-compose.yaml up -d",
    "dev:build:stop": "docker compose -p gtd-xxvi-website-dev -f docker/development/docker-compose.yaml down",
    "dev:build:push": "docker buildx build --platform linux/amd64,linux/arm64 --provenance=mode=max --sbom=true -f docker/development/web.Dockerfile --push -t devpintugtd/gtd-xxvi-website-dev:latest .",
    "\n========== Building ==========": "",
    "build": "next build",
    "build:docker": "docker build -t gtd-xxvi/website -f docker/production/Dockerfile .",
    "start:docker": "docker run --name gtd-xxvi-website -p 3000:3000 --mount type=bind,source=\"/$(pwd)\"/.env.production.local,target=/app/.env.production.local --rm gtd-xxvi/website",
    "push:docker": "docker buildx build --platform linux/amd64,linux/arm64 --provenance=mode=max --sbom=true -f docker/production/Dockerfile --push -t devpintugtd/gtd-xxvi-website:latest .",
    "\n========== Linting ==========": "",
    "lint": "next lint",
    "lint:prettier:check": "prettier . --check",
    "lint:prettier:write": "prettier . --write",
    "\n========== Deployment ==========": "",
    "deploy:fly": "flyctl deploy"
  },
